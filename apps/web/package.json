    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.48.2",
    "@testing-library/react": "^16.3.0",
    "@types/mapbox-gl": "^2.7.14",
    "@types/node": "^24.9.1",
    "@types/react": "^19.1.16",
